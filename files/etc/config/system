config system
        option hostname 'SmartSwitch'
        option timezone 'EET-2EEST,M3.5.0/3,M10.5.0/4'

config timeserver 'ntp'
        list server '0.ro.pool.ntp.org'
        list server '1.ro.pool.ntp.org'
        list server '2.ro.pool.ntp.org'
        list server '3.ro.pool.ntp.org'
        option enable_server '0'

config led 'led_wifi_led'
        option name 'wifi'
        option sysfs 'rt2800pci-phy0::radio'
        option trigger 'netdev'
        option dev 'wlan0'
        option mode 'link tx rx'

